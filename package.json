{
  "name": "cljdoc",
  "scripts": {
    "format": "prettier --write 'js/**/*.{js,jsx,ts,tsx}'",
    "lint": "prettier --check 'js/**/*.{js,jsx,ts,tsx}'",
    "dev": "parcel watch resources/public/cljdoc.html --dist-dir resources-compiled/public/out",
    "build": "parcel build resources/public/cljdoc.html --dist-dir resources-compiled/public/out",
    "typecheck": "npx tsc --watch"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "elasticlunr": "^0.9.5",
    "fuzzysort": "^2.0.4",
    "idb": "^7.1.1",
    "preact": "^10.13.1"
  },
  "devDependencies": {
    "@parcel/packager-xml": "^2.8.3",
    "@parcel/transformer-image": "^2.8.3",
    "@parcel/transformer-xml": "^2.8.3",
    "@types/elasticlunr": "^0.9.4",
    "@types/lunr": "^2.3.4",
    "parcel": "^2.8.2",
    "parcel-reporter-static-files-copy": "^1.5.0",
    "parcel-resolver-ignore": "^2.1.3",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "typescript": "^5.0.4"
  },
  "staticFiles": {
    "staticPath": "resources/public/static"
  },
  "parcelIgnore": [
    "opensearch.xml"
  ]
}
